{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run CMake",
			"type": "shell",
			"command": "${workspaceFolder}\\tools\\run_cmake.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": []
		},
		{
			"label": "Build Game_Angine",
			"type": "shell",
			"command": "cmake",
			"group": "build",
			"args": [
				"--build",
				"build",
				"--config",
				"Debug"
			],
			"problemMatcher": [
				"$msCompile"
			],
			"detail": "Build Game_Angine"
		}
	]
}